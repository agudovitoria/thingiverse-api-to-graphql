{
  "name": "thingiverse-api-to-graphql-server",
  "version": "1.0.0",
  "description": "Thingiverse api to graphql client adapter. Server side.",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=axios node -r ts-node/register ./src/index.ts",
    "start:debug": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agudovitoria/thingiverse-api-to-graphql-server.git"
  },
  "keywords": [
    "thingiverse",
    "api",
    "graphql",
    "rest",
    "server"
  ],
  "author": "I. Agudo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agudovitoria/thingiverse-api-to-graphql-server/issues"
  },
  "homepage": "https://github.com/agudovitoria/thingiverse-api-to-graphql-server#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "axios-debug-log": "^0.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "form-data": "^3.0.0",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/morgan": "^1.9.1",
    "@types/compression": "^1.7.0",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/node": "^14.14.0",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
