{
  "name": "thingiverse-api-to-graphql-server",
  "version": "1.0.0",
  "description": "Thingiverse api to graphql client adapter. Server side.",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=axios node -r ts-node/register ./src/index.ts",
    "start:debug-node": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "start:debug": "nodemon --inspect",
    "build": "tsc",
    "test": "NODE_ENV=test jest --detectOpenHandles --no-cache  --forceExit",
    "test:unit": "ENV_FILE=./config/.env.test jest --detectOpenHandles --forceExit --no-cache --verbose",
    "test:unit:coverage": "ENV_FILE=./config/.env.test jest --coverage --detectOpenHandles --forceExit --no-cache  --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agudovitoria/thingiverse-api-to-graphql-server.git"
  },
  "keywords": [
    "thingiverse",
    "api",
    "graphql",
    "rest",
    "server"
  ],
  "author": "I. Agudo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agudovitoria/thingiverse-api-to-graphql-server/issues"
  },
  "homepage": "https://github.com/agudovitoria/thingiverse-api-to-graphql-server#readme",
  "dependencies": {
    "@types/cors": "^2.8.8",
    "axios": "^0.21.0",
    "axios-debug-log": "^0.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "form-data": "^3.0.0",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "qs": "^6.9.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@jest-mock/express": "^1.1.9",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/jest": "^26.0.15",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.0",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "babel-jest": "^26.6.3",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "jest-express": "^1.12.0",
    "nodemon": "^2.0.5",
    "superagent": "^6.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
